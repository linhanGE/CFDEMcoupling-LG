FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphaHeavy;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
		type            zeroGradient;
    }
	
    outlet
    {
        type            zeroGradient;
    }
	
    walls
    {
        type            zeroGradient;
    }
	
	frontAndBackPlanes
	{
	    type empty;
	}
}

